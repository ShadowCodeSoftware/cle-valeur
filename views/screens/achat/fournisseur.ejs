                              <table class="table table-striped">
                                <thead>
                                  <tr>
                                    <th>
                                      .
                                    </th>
                                    <th>
                                      Fournisseur
                                    </th>
                                    <th>
                                      Contact
                                    </th>
                                    <th>
                                      Localisation
                                    </th>
                                    <th>
                                      Actions
                                    </th>
                                  </tr>
                                </thead>
                                <tbody>
                                  <!-- <%= four%> -->
                                  <% if(four.length > 0) { %>
                                  <% var j = four.length; %>
                                  
                                  <% for(let i=1; i<j; i++){ %>
                                  <tr>
                                    <td class="py-1">
                                      <i class="mdi mdi-grid-large menu-icon"></i>
                                    </td>

                                    <td>
                                      <%= four[i][0] %>
                                    </td>

                                    <td>
                                      <%= four[i][1] %>
                                    </td>

                                    <td>
                                      <%= four[i][2] %>
                                    </td>

                                    <td>
                                      <div class="btn-wrapper">
                                        <a class="btn btn-success text-white me-0" href="/fournisseur/update/<%= four[0][i-1] %>">
