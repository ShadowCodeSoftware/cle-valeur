                                          Modifier
                                        </a>
                                        <a href="/api/fournisseur/del/<%= four[0][i-1] %>" class="btn btn-danger text-white me-0">
                                          Supprimer
                                          <!-- <button onclick="OpenViewWindow()" class="btn btn-danger text-white me-0" href="#">Supprimer</button> -->
                                        </a>
                                      </div>
                                    </td>
                                  </tr>

                                  <% } %>
                                <% } %>
                                  
                                </tbody>
                              </table>
